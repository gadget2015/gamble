<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<name>SeeUsLajv</name>
	<navigation-rule>
		<from-view-id>/startdialog.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/startdialog.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>takeatour</from-outcome>
			<to-view-id>/takeatour.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>newfunctions</from-outcome>
			<to-view-id>/newfunctions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/randomsystemdialog.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/startdialog.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/newfunctions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/startdialog.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/takeatour.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/startdialog.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/activityevents.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/startdialog.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/component/developingLogin.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/myprofile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/component/developingLogin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/resources/authentication/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/myprofile.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>failed</from-outcome>
			<to-view-id>/resources/authentication/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/myprofile.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/myprofile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<locale-config>
			<default-locale>sv_SE</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>text</base-name>
			<var>i18ntext</var>
		</resource-bundle>
	</application>
</faces-config>