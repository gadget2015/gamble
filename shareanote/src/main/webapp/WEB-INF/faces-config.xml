<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	metadata-complete="false">
	<name>ShareNote</name>

	<navigation-rule>
		<from-view-id>/startdialog_goodbrowser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>/searchdialog.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog</from-outcome>
			<to-view-id>/startdialog_goodbrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog_goodbrowser</from-outcome>
			<to-view-id>/startdialog_goodbrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/startdialog_safaribrowser.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>/searchdialog.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog_safaribrowser</from-outcome>
			<to-view-id>/startdialog_safaribrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/searchdialog.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>/searchdialog.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog_goodbrowser</from-outcome>
			<to-view-id>/startdialog_goodbrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog_safaribrowser</from-outcome>
			<to-view-id>/startdialog_safaribrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/footer.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>helpdialog</from-outcome>
			<to-view-id>/helpdialog.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/helpdialog.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>robstartdialog_goodbrowser</from-outcome>
			<to-view-id>/startdialog_goodbrowser.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>robstartdialog_safaribrowser</from-outcome>
			<to-view-id>/startdialog_safaribrowser.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/errormessage_dialog.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>startdialog_goodbrowser</from-outcome>
			<to-view-id>/startdialog_goodbrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
		<navigation-case>
			<from-outcome>startdialog_safaribrowser</from-outcome>
			<to-view-id>/startdialog_safaribrowser.xhtml</to-view-id>
			<redirect /> 
		</navigation-case>
	</navigation-rule>


	<application>
		<locale-config>
			<default-locale>sv_SE</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>text</base-name>
			<var>i18ntext</var>
		</resource-bundle>
	</application>
</faces-config>